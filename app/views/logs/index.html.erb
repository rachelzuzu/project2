<% current_user.logs.each do |log| %> 
	<p> Type : <%=log.kind%> 
		<%=link_to 'View', user_log_path(current_user, log)%>
		<%= link_to 'Delete', user_log_path(current_user, log),
        	method: :delete,
      		data: {confirm: 'Are you sure?'} %> </p>
<%end%> 

<%= form_for [current_user, @log] do |f| %>
<%= f.hidden_field :kind, value: "exercise" %>
<%= f.submit 'New Exercise Log' %>
<% end %>

